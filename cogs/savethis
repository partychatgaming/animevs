                                                        o_used_resolve=True
                                                        o_final_stand=False
                                                        o_used_focus=True
                                                        t_stamina = t_stamina - int(dmg['STAMINA_USED'])                                               
                                                        turn_total= turn_total + 1
                                                        turn=0
                                                    else:
