                                                    #Resolve Scaling
                                                    c_resolve_health = round(fortitude + (.5 * c_resolve))
                                                    c_resolve_attack = round((.30 * c_defense) * (c_resolve / (.50 * c_defense)))
                                                    c_resolve_defense = round((.30 * c_defense) * (c_resolve / (.50 * c_defense)))
