                                style=ButtonStyle.grey,
                                label="Quit | /ff",
                                custom_id = "q"
                            ),
                        ]

                        if o_used_focus and o_used_resolve and not o_pet_used:
                            util_buttons.append(
                                    manage_components.create_button(
                                    style=ButtonStyle.green,
                                    label=" 15",
                                    custom_id = "6"
                                )
                            )
                            
