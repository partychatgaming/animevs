                                    cap1 = list(c_1.values())[0] + ccard_lvl_ap_buff
                                    cap2 = list(c_2.values())[0] + ccard_lvl_ap_buff
                                    cap3 = list(c_3.values())[0] + ccard_lvl_ap_buff
