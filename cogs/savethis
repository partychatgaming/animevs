                                aiMove = 3
                            elif t_stamina >= 70 and (t_health >= o_health):
                                if t_enhancer['TYPE'] in Gamble_Enhancer_Check:
                                    aiMove = 1
                                else:
                                    aiMove = 4
                            elif t_stamina >= 70:
                                aiMove = 1
                            elif t_stamina >= 60 and (t_health >= o_health):
                                if t_used_resolve == False and t_used_focus:
                                    aiMove = 5
                                elif t_used_focus == False:
                                    aiMove = 2
                                else:
                                    aiMove = 1 
