                                    aiMove = 1
                                elif t_stamina >= 40:
                                    aiMove = 2
                                elif t_stamina >= 30 and (t_health >= c_health):
                                    aiMove = 4
                                elif t_stamina >= 30:
                                    aiMove = 2
