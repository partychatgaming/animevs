                                                    o_attack = o_attack + (.50 * o_attack)
                                                    o_defense = o_defense +(.50 * o_defense)
                                                    o_stamina=100
                                                    o_used_resolve=True
                                                    o_final_stand=False
                                                    o_used_focus=True
