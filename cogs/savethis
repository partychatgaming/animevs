                                    aiMove = 4
                                elif t_stamina >= 80 and (t_health >= c_health):
                                    aiMove = 1
                                elif t_stamina >= 80:
                                    aiMove = 3
                                elif t_stamina >= 70 and (t_health >= c_health):
                                    aiMove = 4
                                elif t_stamina >= 70:
                                    aiMove = 1
                                elif t_stamina >= 60 and (t_health >= c_health):
                                    if t_used_resolve == False and t_used_focus:
                                        aiMove = 5
                                    elif t_used_focus == False:
                                        aiMove = 2
                                    else:
                                        aiMove = 1 
                                elif t_stamina >= 60:
                                    if t_used_resolve == False and t_used_focus:
                                        aiMove = 5
                                    elif t_used_focus == False:
                                        aiMove = 2
                                    else:
                                        aiMove = 1 
