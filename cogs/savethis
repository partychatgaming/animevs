                                    manage_components.create_button(
                                        style=ButtonStyle.grey,
                                        label="Block 20",
                                        custom_id = "0"
                                    ),
                                    manage_components.create_button(
                                        style=ButtonStyle.grey,
                                        label="Quit | /ff",
                                        custom_id = "q"
                                    ),
                                ]

